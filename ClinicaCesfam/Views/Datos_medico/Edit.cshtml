@model ClinicaCesfam.Models.Datos_medico

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Datos_medico</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.id_persona)

        @Html.HiddenFor(model => model.id_medico)

        @Html.HiddenFor(model => model.Nombre_Completo)

        @Html.HiddenFor(model => model.Rut)

        @Html.HiddenFor(model => model.Cargo_Medico)

        @Html.HiddenFor(model => model.Telefono)

        @Html.HiddenFor(model => model.Correo)

        @Html.HiddenFor(model => model.Direccion)

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
