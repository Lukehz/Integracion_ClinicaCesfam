<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <!-- Navbar Brand-->
        <a class="navbar-brand ps-3" href="/">Clinica CESFAM</a>
        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>
        <!-- Navbar Search-->
        <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
            <div class="input-group">
                <input class="form-control" type="text" placeholder="Search for..." aria-label="Search for..." aria-describedby="btnNavbarSearch" />
                <button class="btn btn-primary" id="btnNavbarSearch" type="button"><i class="fas fa-search"></i></button>
            </div>
        </form>
        <!-- Navbar-->
        <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="#!">Settings</a></li>
                    <li><a class="dropdown-item" href="#!">Activity Log</a></li>
                    <li><hr class="dropdown-divider" /></li>
                    <li><a class="dropdown-item" href="#!">Logout</a></li>
                </ul>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">Vistas</div>
                        <a class="nav-link" href="Datos_paciente">
                            <div class="sb-nav-link-icon"><i class="fas fa-user-injured fa-lg"></i></div>
                            Datos Pacientes
                        </a>

                        <a class="nav-link" href="Datos_medico">
                            <div class="sb-nav-link-icon"><i class="fas fa-user-md fa-lg"></i></div>
                            Datos Medicos
                        </a>

                        <a class="nav-link" href="Stock_medicamentos">
                            <div class="sb-nav-link-icon"><i class="fas fa-boxes fa-lg"></i></div>
                            Stock Medicamentos
                        </a>
                        <div class="sb-sidenav-menu-heading">Atencion</div>
                        <a class="nav-link collapsed" href="#" data-bs-toggle="collapse" data-bs-target="#collapseLayouts" aria-expanded="false" aria-controls="collapseLayouts">
                            <div class="sb-nav-link-icon"><i class="fas fa-notes-medical fa-lg"></i></div>
                            Consulta
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayouts" aria-labelledby="headingOne" data-bs-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" href="Hoja_atencion">Hoja atencion</a>
                                <a class="nav-link" href="Receta_medicamento">Receta medicamento</a>
                            </nav>
                        </div>
                        <a class="nav-link collapsed" href="#" data-bs-toggle="collapse" data-bs-target="#collapsePages" aria-expanded="false" aria-controls="collapsePages">
                            <div class="sb-nav-link-icon"><i class="fas fas fa-prescription-bottle fa-lg"></i></div>
                            Medicamentos
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapsePages" aria-labelledby="headingTwo" data-bs-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" href="Medicamentoes">Lista Medicamentos</a>
                                <a class="nav-link" href="Reservas">Reserva Medicamentos</a>
                            </nav>
                        </div>
                        <div class="sb-sidenav-menu-heading">Usuarios</div>
                        <a class="nav-link" href="Personas">
                            <div class="sb-nav-link-icon"><i class="fas fa-users fa-lg"></i></div>
                            Personas
                        </a>
                        <a class="nav-link" href="Pacientes">
                            <div class="sb-nav-link-icon"><i class="fas fa-hospital-user fa-lg"></i></div>
                            Pacientes
                        </a>
                        <a class="nav-link" href="Medicos">
                            <div class="sb-nav-link-icon"><i class="fas fa-hospital-user fa-lg"></i></div>
                            Medicos
                        </a>
                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <a class="nav-link" href="Carro"><strong>Transbank</strong></a>
                    <div class="small">Logged in as:</div>
                    Start Bootstrap
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    @RenderBody()
                </div>
            </main>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid px-4">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright &copy; Your Website 2023</div>
                        <div>
                            <a href="#">Privacy Policy</a>
                            &middot;
                            <a href="#">Terms &amp; Conditions</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>


    @*<li>@Html.ActionLink("Personas", "Index", "PERSONAs")</li>
        <li>@Html.ActionLink("Pacientes", "Index", "PACIENTEs")</li>
        <li>@Html.ActionLink("Medico", "Index", "Medicos")</li>
        <li>@Html.ActionLink("Medicamentos", "Index", "MEDICAMENTOes")</li>
        <li>@Html.ActionLink("Datos paciente", "Index", "Datos_paciente")</li>
        <li>@Html.ActionLink("Datos medico", "Index", "Datos_medico")</li>
        <li>@Html.ActionLink("stock medicamentos", "Index", "Stock_medicamentos")</li>*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/complementos")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
